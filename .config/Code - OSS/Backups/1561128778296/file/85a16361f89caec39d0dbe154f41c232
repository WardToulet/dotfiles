#!/bin/sh
xrandr ---output eDP-1-1 --pr--output eDP-1-1 --primary --output HDMI-0 --offimary --output HDMI-0 --off-output HDMI-0 --off --output eDP-1-1 --mode 1920x1080 --pos 0x0 --rotate normal --output HDMI-1-1 --off
